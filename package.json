{
  "name": "joga",
  "version": "0.18.1",
  "main": "./dist/joga.umd.js",
  "module": "./dist/joga.es.js",
  "exports": {
    ".": {
      "import": "./dist/joga.es.js",
      "require": "./dist/joga.umd.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "bin": "./cli/index.js",
  "scripts": {
    "build": "tsc && vite build",
    "cli": "joga",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch",
    "update": "npx npm-check-updates -u && npm i && npm t"
  },
  "lint-staged": {
    "*.{js,ts,md,mdx,html,json}": "prettier --write"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@emotion/css": "^11.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@mdx-js/react": "^2.1.1",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-interactions": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@tsconfig/vite-react": "^1.0.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-dts": "^1.0.4"
  }
